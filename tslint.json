{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "compilerOptions": {
    "plugins": [
      { "name": "tslint-language-service"}
    ]
  },
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "no-console": false,
    "prettier": true,
    "quotemark": [true, "single"]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
